{
	"name": "nest-graphql-sequelize",
	"version": "0.11.2",
	"description": "",
	"author": "",
	"private": true,
	"license": "MIT",
	"scripts": {
		"clean": "rm -rf ./dist && rm -rf tsconfig.tsbuildinfo",
		"prebuild": "rimraf dist",
		"build": "tsc -p tsconfig.build.json",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "APP_ROOT_PATH=. nest start --watch",
		"start:prod": "node dist/main"
	},
	"dependencies": {
		"@nestjs-query/core": "0.11.2",
		"@nestjs-query/query-graphql": "0.11.2",
		"@nestjs-query/query-sequelize": "0.11.2",
		"@nestjs/common": "7.0.11",
		"@nestjs/core": "7.0.11",
		"@nestjs/graphql": "7.3.9",
		"@nestjs/platform-express": "7.0.11",
		"@nestjs/sequelize": "0.1.0",
		"apollo-server-express": "2.13.1",
		"graphql": "15.0.0",
		"graphql-tools": "5.0.0",
		"pg": "8.2.1",
		"reflect-metadata": "0.1.13",
		"rimraf": "3.0.2",
		"rxjs": "6.5.5",
		"sequelize": "5.21.8",
		"sequelize-typescript": "1.1.0"
	},
	"devDependencies": {
		"@nestjs/cli": "7.1.5",
		"@nestjs/schematics": "7.0.0",
		"@nestjs/testing": "7.0.11",
		"@types/express": "4.17.6",
		"@types/jest": "25.2.2",
		"@types/node": "13.13.5",
		"@types/supertest": "2.0.9",
		"jest": "26.0.1",
		"prettier": "2.0.5",
		"supertest": "4.0.2",
		"ts-jest": "26.0.0",
		"ts-loader": "7.0.4",
		"ts-node": "8.10.1",
		"tsconfig-paths": "3.9.0",
		"typescript": "3.8.3"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".spec.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"coverageDirectory": "./coverage",
		"testEnvironment": "node"
	}
}
