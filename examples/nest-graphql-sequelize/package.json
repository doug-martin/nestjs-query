{
	"name": "nest-graphql-sequelize",
	"version": "0.11.8",
	"description": "",
	"author": "",
	"private": true,
	"license": "MIT",
	"scripts": {
		"clean": "rm -rf ./dist && rm -rf tsconfig.tsbuildinfo",
		"prebuild": "rimraf dist",
		"build": "tsc -p tsconfig.build.json",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "APP_ROOT_PATH=. nest start --watch",
		"start:prod": "node dist/main"
	},
	"dependencies": {
		"@nestjs-query/core": "0.11.8",
		"@nestjs-query/query-graphql": "0.11.8",
		"@nestjs-query/query-sequelize": "0.11.8",
		"@nestjs/common": "7.1.2",
		"@nestjs/core": "7.1.2",
		"@nestjs/graphql": "7.4.1",
		"@nestjs/platform-express": "7.1.2",
		"@nestjs/sequelize": "0.1.0",
		"apollo-server-express": "2.14.1",
		"class-validator": "0.12.2",
		"graphql": "15.0.0",
		"graphql-tools": "6.0.5",
		"pg": "8.2.1",
		"rimraf": "3.0.2",
		"rxjs": "6.5.5",
		"sequelize": "5.21.11",
		"sequelize-typescript": "1.1.0"
	},
	"devDependencies": {
		"@nestjs/cli": "7.2.0",
		"@nestjs/schematics": "7.0.1",
		"@nestjs/testing": "7.1.2",
		"@types/express": "4.17.6",
		"@types/jest": "25.2.3",
		"@types/node": "13.13.5",
		"@types/supertest": "2.0.9",
		"jest": "26.0.1",
		"prettier": "2.0.5",
		"supertest": "4.0.2",
		"ts-jest": "26.1.0",
		"ts-loader": "7.0.5",
		"ts-node": "8.10.2",
		"tsconfig-paths": "3.9.0",
		"typescript": "3.9.3"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".spec.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"coverageDirectory": "./coverage",
		"testEnvironment": "node"
	}
}
