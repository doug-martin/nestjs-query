{
  "name": "nest-graphql-typeorm-multidb",
  "version": "0.10.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf ./dist && rm -rf tsconfig.tsbuildinfo",
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "APP_ROOT_PATH=. nest start --watch",
    "start:prod": "node dist/main",
    "reset": "npm run build && typeorm schema:drop && npm run schema:sync",
    "schema:sync": "typeorm schema:sync"
  },
  "dependencies": {
    "@nestjs-query/core": "0.10.0",
    "@nestjs-query/query-graphql": "0.10.0",
    "@nestjs-query/query-typeorm": "0.10.0",
    "@nestjs/common": "7.0.9",
    "@nestjs/core": "7.0.9",
    "@nestjs/graphql": "7.3.4",
    "@nestjs/platform-express": "7.0.9",
    "@nestjs/typeorm": "7.0.0",
    "apollo-server-express": "2.12.0",
    "graphql": "15.0.0",
    "graphql-tools": "5.0.0",
    "pg": "8.0.3",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.5.5",
    "typeorm": "0.2.24",
    "typeorm-seeding": "1.6.1"
  },
  "devDependencies": {
    "@nestjs/cli": "7.1.4",
    "@nestjs/schematics": "7.0.0",
    "@nestjs/testing": "7.0.9",
    "@types/express": "4.17.6",
    "@types/jest": "25.2.1",
    "@types/node": "13.13.4",
    "@types/supertest": "2.0.8",
    "jest": "25.5.2",
    "prettier": "2.0.5",
    "supertest": "4.0.2",
    "ts-jest": "25.4.0",
    "ts-loader": "7.0.2",
    "ts-node": "8.9.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
